#: chathans.py:69
msgid "ASCII File : "
msgstr "ആസ്കി ഫയല്‍"

#: chathans.py:70
msgid "Mapping File : "
msgstr "മാപ്പിങ്ങ് ഫയല്‍"

#: chathans.py:71
msgid "Unicode File : "
msgstr "യൂണിക്കോഡ് ഫയല്‍"

#: chathans.py:74
