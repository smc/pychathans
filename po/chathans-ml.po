#: chathans.py:85
msgid "Quit"
msgstr "പുറത്തുകടക്കുക"

#: chathans.py:87
msgid "About"
msgstr "സംബന്ധിച്ച്"

#: chathans.py:149
msgid "Please select both ASCII file and Mapping file"
msgstr "ദയവായി ആസ്കി ഫയലും മാപ്പിങ്ങ് ഫയലും തിരഞ്ഞെടുക്കുക"

#: chathans.py:163
msgid "Coversion Done - Unicode file :"
